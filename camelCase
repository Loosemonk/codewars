public class Solution {

    public static String camelCase(String str) {
      String s = str.trim(); 
      String[] camel = s.split("\\s+");
      String newCamel = ""; 
      String camelCamel = "";
     if(str.isEmpty()){
       return str;
     }else
      for(int i = 0 ; i < camel.length; i++){
           String camelCase = camel[0];
  String samelCase = camel[i].substring(0,1).toUpperCase().concat(camel[i].substring(1));;
        camelCamel = newCamel + samelCase;
       newCamel = camelCamel;
           }
      return newCamel;
    }

}